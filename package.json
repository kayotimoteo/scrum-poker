{
  "name": "scrum-poker",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "dotenv -e .env.local -- turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "dependencies": {
    "api": "*",
    "dotenv-cli": "^6.0.0",
    "eslint-config-custom": "*",
    "tsconfig": "*",
    "web": "*"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "eslint-config-custom": "*",
    "postcss": "^8.4.23",
    "prettier": "latest",
    "tailwindcss": "^3.3.2",
    "@headlessui/react": "^1.7.14",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.19"
}
